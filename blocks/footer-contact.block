<ul class="u-margin-hug-top u-list-unstyled u-text-center container__item">
  {.repeated section items}
    <!-- external link -->
    {.section externalLink}
      <li>
        <h3 class="h4">{title}</h3>
        <p>
          <a href="{url}">
            <script type="text/javascript">
              document.write({url|json}.replace('mailto:', ''))
            </script>
          </a>
        </p>
      </li>
    {.end}
  {.end}
</ul>
